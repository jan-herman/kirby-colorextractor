{
    "name": "jan-herman/kirby-colorextractor",
    "description": "Extract dominant / average color from any image.",
    "version": "1.0.2",
    "type": "kirby-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jan-herman/kirby-colorextractor",
    "authors": [
        {
            "name": "Sylvain Jul√©",
            "email": "contact@sylvain-jule.fr"
        },
        {
            "name": "Jan Herman",
            "email": "mail@janherman.com",
            "homepage": "https://janherman.com"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.2",
        "ksubileau/color-thief-php": "^2.0"
    },
    "extra": {
        "installer-name": "colorextractor"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "JanHerman\\ColorExtractor\\": "lib/"
        }
    }
}
